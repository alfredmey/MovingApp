<%= simple_nested_form_for(@shipment) do |f| %>
    <%= f.error_notification %>
    <div>
      <div class="form-group string shipment_pack_date">
        <label class="string control-label" for="shipment_pack_date">Pack Date</label>
        <%= f.text_field :pack_date, :class => 'datepicker input-sm form-control', required: false  %>
      </div>
      <div class="form-group string shipment_pick_date">
        <label class="string control-label" for="shipment_pick_date">Pick Date</label>
        <%= f.text_field :pick_date, :class => 'datepicker input-sm form-control', required: false  %>
      </div>
      <div class="form-group string required shipment_deliver_date">
        <label class="string control-label" for="shipment_deliver_date">Deliver Date</label>
        <%= f.text_field :deliver_date, :class => 'datepicker input-sm form-control', required: false  %>
      </div>
      <div class="form-group string required shipment_survey_date">
        <label class="string control-label" for="shipment_survey_date">Survey Date</label>
        <%= f.text_field :survey_date, :class => 'datepicker input-sm form-control', required: false  %>
      </div>
      <div class="form-group string required shipment_scan_date">
        <label class="string control-label" for="shipment_scan_date">Scan Date</label>
        <%= f.text_field :scan_date, :class => 'datepicker input-sm form-control', required: false  %>
      </div>
    </div>
    <div class="form-actions">
      <%= link_to 'Back', shipments_path, :class => 'btn btn-danger' %><%= f.button :submit, :class => 'pull-right btn-success' %>
    </div>
<% end %>